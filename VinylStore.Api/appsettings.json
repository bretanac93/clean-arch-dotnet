{
  "ConnectionStrings": {
    "PostgresConnection": "Host={DB_HOST};Port={DB_PORT};Database={DB_NAME};User ID={DB_USER};Password={DB_PASSWORD};"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JwtSettings": {
    "Key": "{APP_SECRET}",
    "Issuer": "VinylStore",
    "Audience": "BaseUser"
  },
  "AllowedHosts": "*"
}
